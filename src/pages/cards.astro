---
import PageMeta from "../components/PageMeta.astro";
import PageLayout from "../layouts/PageLayout.astro";

import { SITE_TITLE } from "../config";
import Footer from "../components/Footer.astro";
import MyCards from "src/components/Flashcards/App.svelte";
---

<PageLayout>
	<PageMeta title={`Flashcards | ${SITE_TITLE}`} slot="meta" />
	<Fragment slot="main">
		<MyCards client:load />
	</Fragment>
</PageLayout>
<Footer />
